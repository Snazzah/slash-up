  "funding": {
    "url": "https://github.com/sponsors/Snazzah"
  },
  "bin": {
    "slash-up": "./lib/bin.js"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "ansi-colors": "^4.1.3",
    "common-tags": "^1.8.2",
    "cross-spawn": "^7.0.3",
    "degit": "^2.8.4",
    "dotenv": "^16.0.3",
    "enquirer": "^2.3.6",
    "find-up": "5.0.0",
    "log-symbols": "4.1.0",
    "slash-create": "^6.0.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/degit": "^2.8.3",
    "@types/node": "^18.15.0",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^4.4.0",
    "shx": "^0.3.4",
    "tsx": "^3.12.3",
    "typescript": "^5.0.2"
  }
