{
  "name": "slash-up",
  "version": "1.0.4",
  "description": "CLI for viewing and syncing Discord commands with slash-create",
  "author": "Snazzah",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/Snazzah/slash-up",
  "bugs": {
    "url": "https://github.com/Snazzah/slash-up/issues"
  },
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "slash-create",
    "slash-command",
    "slash-commands",
    "discord-interactions",
    "cli"
  ],
  "scripts": {
    "build": "npx rimraf lib && npx tsc",
    "changelog": "npx ts-node scripts/changelog",
    "lint": "npx eslint --ext .ts ./src",
    "lint:fix": "npx eslint --ext .ts ./src --fix",
    "gpr": "npx ts-node scripts/gpr",
    "prepare": "npx shx test -d ./lib || npm run build",
    "prepublishOnly": "(npx shx test -d ./lib || (echo \"lib folder does not exist\" && exit 1)) && npm run lint:fix"
  },
  "funding": {
    "url": "https://github.com/sponsors/Snazzah"
  },
  "bin": {
    "slash-up": "./lib/bin.js"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@adonisjs/require-ts": "^2.0.8",
    "ansi-colors": "^4.1.1",
    "common-tags": "^1.8.2",
    "cross-spawn": "^7.0.3",
    "degit": "^2.8.4",
    "dotenv": "^10.0.0",
    "enquirer": "^2.3.6",
    "find-up": "5.0.0",
    "log-symbols": "4.1.0",
    "slash-create": "^4.4.2",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/degit": "^2.8.3",
    "@types/node": "^17.0.1",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
